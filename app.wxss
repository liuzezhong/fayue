/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.coarse-line {
  width: 750rpx;
  height: 20rpx;
  background-color: #f5f5f5;
}

.project-recommend {
  width: 750rpx;
}

.project-line {
  width: 750rpx;
  height: 20rpx;
  background-color: #f5f5f5;
}

.project-header {
  margin-top: 30rpx;
  margin-left: 30rpx;
  margin-bottom: 30rpx;
  width: 690rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.project-header-title {
  color: #010101;
  font-size: 34rpx;
  font-weight: bold;
  width: 590rpx;
  display: flex;
  align-items: center;
  display: flex;
  align-items: center;
}

.project-header-title-view {
  width: 8rpx;
  height: 35rpx;
  background-color: #ffd900;
  margin-right: 15rpx;
}

.project-header-title image {
  width: 42rpx;
  height: 46rpx;
  margin-right: 20rpx;
}

.project-header-subtitle {
  color: #b3b3b3;
  font-size: 30rpx;
  display: flex;
  align-items: center;
}

.project-header-subtitle image {
  width: 14rpx;
  height: 23rpx;
  padding-left: 20rpx;
}

.project-main {
  margin-bottom: 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.project-list {
  width: 360rpx;
  white-space: normal;
  display: flex;
  flex-direction: column;
  margin-left: 30rpx;
}

.project-list-image {
  width: 360rpx;
  height: 210rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.project-list-image image {
  width: 360rpx;
  height: 210rpx;
  border-radius: 20rpx;
}

.project-list-title {
  width: 360rpx;
  height: 80rpx;
  margin-top: 20rpx;
  color: #010101;
  line-height: 35rpx;
  font-size: 28rpx;
}

.scroll-view_H {
  white-space: nowrap;
  display: flex;
  flex-direction: row;
}

.view-item {
  display: inline-block;
}

.bottom-line {
  height: 150rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.bottom-line-content {
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0rpx 20rpx 0rpx 20rpx;
}

.bottom-line-content text {
  color: #b2b2b2;
  font-size: 22rpx;
}

.bottom-line-left {
  width: 150rpx;
  height: 1rpx;
  background-color: #e5e5e5;
}

.bottom-line-right {
  width: 150rpx;
  height: 1rpx;
  background-color: #e5e5e5;
}

.data-line {
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  width: 690rpx;
  display: flex;
  flex-direction: row;
  align-items: center;

}

.data-line .view {
  color: #808080;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  width: 160rpx;
}

.data-line .view image{
  width: 32rpx;
  height: 18rpx;
  padding-right: 15rpx;

}

.data-line .like {
  color: #808080;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  
}

.data-line .like image{
  width: 25rpx;
  height: 25rpx;
  padding-right: 15rpx;
}

.label-line {
  margin-top: 30rpx;
  width: 690rpx;
  color: #808080;
  font-size: 24rpx;
}

.wiki-line {
  width: 690rpx;
  height: 1rpx;
  background-color: #E5E5E5;
}

.wiki-line-s {
  width: 670rpx;
  height: 1rpx;
  background-color: #E5E5E5;
}








.wiki-recommend {
  margin-top: 50rpx;
  margin-left: 30rpx;
  margin-bottom: 40rpx;
  width: 690rpx;
}

.wiki-header {
  display: flex;
  flex-direction: row;
}

.wiki-header-title {
  width: 550rpx;
  display: flex;
}

.wiki-header-title-left {
  color: #333333;
  font-size: 36rpx;
  font-weight: bold;
  width: 550rpx;
  display: flex;
  align-items: center;
}

.wiki-header-title-right {
  display: flex;
  align-items: center;
  justify-content: center;
}

.refush-image {
  width: 28rpx;
  height: 28rpx;
  
}

.wiki-header-title-right text{
  color: #b2b2b2;
  font-size: 28rpx;
  margin-left: 15rpx;
}

.wiki-header-title-view {
  width: 8rpx;
  height: 35rpx;
  background-color: #ffd900;
  margin-right: 20rpx;
}

.wiki-header-title image {
  width: 42rpx;
  height: 46rpx;
  margin-right: 20rpx;
}

.wiki-header-subtitle {
  color: #b3b3b3;
  font-size: 30rpx;
  display: flex;
  align-items: center;
}

.wiki-header-subtitle image {
  width: 14rpx;
  height: 23rpx;
  padding-left: 20rpx;
}

.wiki-main {
  margin-top: 20rpx;
  margin-bottom: 30rpx;
  display: flex;
  flex-direction: column;
}

.wiki-recommend-list {
  margin-top: 30rpx;
}

.wiki-recommend-content {
  color: #010101;
  font-size: 32rpx;
  line-height: 52rpx;
}

.return-pages {
  position: fixed;
  right: 40rpx;
  bottom: 180rpx;
}

.return-pages image{
  width: 85rpx;
  height: 85rpx;
}

.under-area {
  width: 750rpx;
  height: 100rpx;
}

.login-marks {
  z-index: 666;
  width: 100%;
  height: 100%;
  background-color: black;
  opacity: 0.8;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
}

.login-bullet {
  width: 750rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  position: fixed;
  z-index: 999;
  top: 0rpx;
  left: 0rpx;
}

.login-bullet .main {
  background-color: #fff;
  width: 660rpx;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.login-bullet .main .close-bullet {
  width: 600rpx;
  display: flex;
  justify-content: flex-end;
  margin-top: 30rpx;
}

.login-bullet .main .close-bullet image {
  width: 25rpx;
  height: 25rpx;
}

.login-bullet .main .header {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  margin-top: -5rpx;
}

.login-bullet .main .header .image {
  width: 102rpx;
  height: 102rpx;
}

.login-bullet .main .header .image image {
  width: 102rpx;
  height: 102rpx;
}

.login-bullet .main .header .text {
  font-size: 36rpx;
  color: #000;
  font-weight: bold;
  margin-top: 30rpx;
}

.login-bullet .main .content-group {
  margin-top: 40rpx;
  width: 560rpx;
  display: flex;
  flex-direction: column;
  border-top: 1rpx solid #d2d3d5;
  border-bottom: 1rpx solid #d2d3d5;
}

.login-bullet .main .content-group .info-line {
  display: flex;
  align-items: center;
  margin-top: 70rpx;
  width: 560rpx;
}

.login-bullet .main .content-group .last-line {
  margin-bottom: 60rpx;
}

.login-bullet .main .content-group .info-line .image {
  width: 80rpx;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.login-bullet .main .content-group .info-line .image image {
  width: 80rpx;
  height: 80rpx;
}

.login-bullet .main .content-group .info-line .title-group {
  margin-left: 30rpx;
  display: flex;
  flex-direction: column;
  width: 300rpx;
}

.login-bullet .main .content-group .info-line .title-group .content-title {
  color: #333;
  font-size: 32rpx;
}

.login-bullet .main .content-group .info-line .title-group .sub-title {
  color: #808080;
  font-size: 26rpx;
  margin-top: 5rpx;
}

.login-bullet .main .content-group .info-line .auth-button {
  margin-left: 10rpx;
}

.login-bullet .main .content-group .info-line .auth-button-noclick {
  margin-left: 10rpx;
}

.login-bullet .main .content-group .info-line .auth-button button {
  width: 140rpx;
  height: 60rpx;
  font-size: 28rpx;
  background-color: #50b674;
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
}

.login-bullet .main .content-group .info-line .auth-button-noclick button {
  width: 140rpx;
  height: 60rpx;
  font-size: 28rpx;
  background-color: #cccccc;
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
}

.login-bullet .main .content-group .info-line .auth-button button::after {
  border: 0rpx;
}

.login-bullet .main .content-group .info-line .auth-button-noclick button::after {
  border: 0rpx;
}

.login-bullet .main .next-step {
  width: 220rpx;
  height: 70rpx;
  background-color: rgba(80, 182, 116, 1);
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: white;
  font-size: 30rpx;
  margin-top: 50rpx;
}

.login-bullet .main .next-step-noclick {
  width: 220rpx;
  height: 70rpx;
  background-color: #cccccc;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: white;
  font-size: 30rpx;
  margin-top: 50rpx;
}


.login-bullet .main .under-area {
  width: 100%;
  height: 50rpx;
}
